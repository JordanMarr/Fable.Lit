{
    "private": "true",
    "scripts": {
        "postinstall": "dotnet tool restore && sh scripts/copy-assets.sh",
        "watch": "dotnet fable watch src -s -o out --run webpack --watch",
        "serve": "live-server public",
        "watch:serve": "concurrently \"npm:watch\" \"npm:serve\"",
        "build": "NODE_ENV=production dotnet fable src -o out --run webpack",
        "deploy": "npm run build && gh-pages -d public"
    },
    "devDependencies": {
        "@capacitor/cli": "^3.2.2",
        "concurrently": "^6.2.1",
        "css-loader": "^5.1.1",
        "file-loader": "^6.2.0",
        "gh-pages": "^3.1.0",
        "live-server": "^1.2.1",
        "resolve-url-loader": "^3.1.2",
        "source-map-loader": "^3.0.0",
        "style-loader": "^2.0.0",
        "webpack": "^5.24.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "dependencies": {
        "@capacitor/android": "^3.2.2",
        "@capacitor/core": "^3.2.2",
        "@ionic/core": "^5.7.0"
    }
}
